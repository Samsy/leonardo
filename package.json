{
  "name": "leonardo",
  "version": "0.0.0",
  "description": "Personal webgl toolkit.",
  "main": "lib/index.js",
  "scripts": {
    "start": "concurrently 'npm run watch' 'npm run server' 'webpack --progress --colors --watch'",
    "server": "live-server ./examples --port 3000 --quiet --watch ./examples --watch ./lib --wait 0",
    "watch": "babel --watch src --out-dir lib",
    "build": "babel src --out-dir lib"
  },
  "author": "David Paul Rosser <info@davidpaulrosser.co.uk> (http://ivxvixviii.io/)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-module-alias": "^1.2.0",
    "babel-preset-es2015": "^6.6.0",
    "concurrently": "^2.0.0",
    "dat-gui": "^0.5.0",
    "live-server": "^0.9.2",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "gl-matrix": "^2.3.2",
    "glslify": "^5.0.2"
  }
}
